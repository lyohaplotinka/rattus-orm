{
  "name": "@rattus-orm/angular-rxjs",
  "description": "ORM for your JS/TS apps: Angular RxJS bindings",
  "homepage": "https://orm.rattus.dev/",
  "author": "Lyoha Plotinka",
  "license": "MIT",
  "packageManager": "yarn@4.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyohaplotinka/rattus-orm.git"
  },
  "bugs": {
    "url": "https://github.com/lyohaplotinka/rattus-orm/issues"
  },
  "keywords": [
    "orm",
    "rattus",
    "angular",
    "rxjs",
    "typescript",
    "javascript"
  ],
  "version": "0.2.1",
  "type": "module",
  "scripts": {
    "ng": "ng",
    "build": "yarn install && ng build",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .angular",
    "lint": "run -T eslint 'src/**/*.{ts,tsx}' --quiet --fix",
    "test": "run -T vitest run",
    "typecheck": "bash -c 'run -T tsc -p tsconfig.json --noEmit'"
  },
  "devDependencies": {
    "@analogjs/platform": "^1.4.0",
    "@analogjs/vite-plugin-angular": "^0.2.45",
    "@angular-devkit/build-angular": "^17.3.8",
    "@angular/cli": "^17.3.8",
    "@angular/common": "^17.3.11",
    "@angular/compiler": "^17.3.11",
    "@angular/compiler-cli": "^17.3.11",
    "@angular/core": "^17.3.11",
    "@angular/platform-browser": "^17.3.11",
    "@angular/platform-browser-dynamic": "^17.3.11",
    "@ngtools/webpack": "^17.3.8",
    "@nx/vite": "^18.3.5",
    "@rattus-orm/core": "workspace:^",
    "@rattus-orm/plugin-zod-validate": "workspace:^",
    "@types/jsdom": "^21.1.7",
    "jsdom": "^24.1.1",
    "ng-packagr": "^17.3.0",
    "rxjs": "~7.8.1",
    "tslib": "^2.6.3",
    "typescript": "~5.4.5",
    "zone.js": "~0.14.10"
  },
  "peerDependencies": {
    "@angular/common": ">=17.0.0",
    "@angular/core": ">=17.0.0",
    "@rattus-orm/core": "^0.4.0"
  },
  "sideEffects": false,
  "rattusMeta": {
    "title": "Angular + RxJS",
    "publishDir": "./dist"
