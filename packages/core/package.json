{
  "name": "@rattus-orm/core",
  "version": "0.0.1",
  "description": "Data-source agnostic ORM for TypeScript",
  "main": "dist/rattus-orm-core.js",
  "browser": "dist/rattus-orm-core.mjs",
  "module": "dist/rattus-orm-core.mjs",
  "typings": "dist/src/rattus-orm-core.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist coverage temp .nyc_output .tmp",
    "lint": "eslint 'src/**/*.ts' --quiet --fix",
    "test": "vitest run",
    "prepare": "npx simple-git-hooks",
    "typecheck": "bash -c 'tsc -p tsconfig.json --noEmit'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyohaplotinka/rattus-orm-new.git"
  },
  "keywords": [
    "ratus",
    "agnostic",
    "orm"
  ],
  "author": "Lyoha Plotinka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyohaplotinka/rattus-orm-new/issues"
  },
  "dependencies": {
    "@types/uuid": "^9.0.7",
    "lint-staged": "^15.0.2",
    "normalizr": "^3.6.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^6.2.0",
    "@typescript-eslint/parser": "^6.2.0",
    "eslint": "^8.46.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "jsdom": "^22.1.0",
    "prettier": "^3.0.0",
    "simple-git-hooks": "^2.8.1",
    "tslib": "^2.6.2",
    "tsup": "^7.2.0",
    "typescript": "^5.2.2",
    "vitest": "^0.34.6"
  },
  "lint-staged": {
    "*.ts": [
      "yarn typecheck",
      "yarn lint",
      "yarn test -- --passWithNoTests"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "./node_modules/.bin/lint-staged"
  }
}
