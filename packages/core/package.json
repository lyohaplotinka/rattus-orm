{
  "name": "@rattus-orm/core",
  "version": "0.0.1",
  "description": "Data-source agnostic ORM for TypeScript",
  "main": "./dist/rattus-orm-core.js",
  "module": "./dist/rattus-orm-core.mjs",
  "types": "./dist/rattus-orm-core.d.ts",
  "exports": {
    ".": {
      "import": "./dist/rattus-orm-core.mjs",
      "require": "./dist/rattus-orm-core.js",
      "types": "./dist/rattus-orm-core.d.ts"
    },
    "./object-data-provider": {
      "import": "./dist/object-data-provider.mjs",
      "require": "./dist/object-data-provider.js",
      "types": "./dist/object-data-provider.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "lint": "eslint 'src/**/*.ts' --quiet --fix",
    "test": "vitest run",
    "typecheck": "bash -c 'tsc -p tsconfig.json --noEmit'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lyohaplotinka/rattus-orm-new.git"
  },
  "keywords": [
    "ratus",
    "agnostic",
    "orm"
  ],
  "author": "Lyoha Plotinka",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/lyohaplotinka/rattus-orm-new/issues"
  },
  "dependencies": {
    "normalizr": "^3.6.2"
  }
}
